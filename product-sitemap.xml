---
layout : null
---
<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet type="text/xsl" href="{{ site.url }}/sitemaps.xsl"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd http://www.google.com/schemas/sitemap-image/1.1 http://www.google.com/schemas/sitemap-image/1.1/sitemap-image.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
 {% for post in site.posts %}
  {% assign i = forloop.index | plus: 10 %}
  
  {% assign TahunNormal = "now" | date: "%Y" %}
  {% assign HariNormal = "now" | date: "%d" %}
  {% assign BulanNormal = "now" | date: "%m" %}
  
  {% assign Hasilo =  site.time | plus: i | date_to_xmlschema%}

  {% assign TahunNggak = Hasilo | date: "%Y" %}
  {% assign HariNggak = Hasilo | date: "%d" %}
  {% assign caraI = Hasilo | replace: TahunNggak , TahunNormal %}
  {% assign caraII = caraI | replace: HariNggak , HariNormal  %}
  {% assign akhirnyaaaaaaaaa = caraII  %}
  
    <url>
        <loc>{{site.url}}{{ post.url | remove: 'index.html' }}</loc>
               <lastmod>{{ akhirnyaaaaaaaaa }}</lastmod>
        <image:image>
                <image:loc>{{ post.image }}</image:loc>
        </image:image>
<changefreq>weekly</changefreq>
<priority>0.8</priority>
    </url>
    {% endfor %}

</urlset>
